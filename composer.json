{
  "name": "oscillas/sanctum-tokens",
  "description": "A Laravel Nova resource tool for sanctum tokens.",
  "keywords": [
    "laravel",
    "nova",
    "sanctum"
  ],
  "license": "MIT",
  "require": {
    "laravel/nova": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Jeffbeltran\\SanctumTokens\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jeffbeltran\\SanctumTokens\\ToolServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.14"
  }
}
